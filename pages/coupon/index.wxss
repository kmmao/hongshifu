/* pages/coupon/index.wxss */
page{background-color: #ccc;}
.bgimg{
  width: 100%;
  height: 100%;
  position: relative;
  z-index: -1;
}
.psty1{
  position: absolute;
  color: #fff;
  top:33%;
  background: url(data:image/png;base64,) no-repeat top center;
background-size:100%;
margin-left: 60rpx;
margin-right: 60rpx;
width:calc( 100% - 120rpx);
height: 600rpx;
box-sizing:border-box;
font-size: 28rpx;
}
.psty2{
  margin-top: 160rpx;
  margin-left: 80rpx;
  margin-right: 80rpx;
  text-align: center;
}
.psty2 input{
font-size: 28rpx;
line-height: 85rpx;
background-color: #fff; 
color: #999999;
border-radius: 4px;
height: 85rpx;}
.btn1{
  font-size: 36rpx;
  background-color: #e7453f;
  border-radius: 40px;
	border: solid 3px #feddd8;
  display: block;
  color: #fff;
  margin-top: 55rpx;
  }
.psty3{
  font-size: 32rpx;
  font-weight:bold;
  text-align:center;
  margin-top: 60rpx;
}
.text1{
    position: relative;
    display: inline-block;
}
.text1::before,.text1::after{
  content: " ";
  position:absolute;
  width: 200rpx;
  height: 2rpx;
  background-color:currentColor;
  top: 22rpx;
}
.text1::before{left: -250rpx;}
.text1::after{right: -250rpx;}
.psty4{
  margin-top: 55rpx;}
.psty5{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}