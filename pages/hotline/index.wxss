/* pages/hotline/index.wxss */
.page1{
  padding: 30rpx;
  font-size: 26rpx;
}
.btn1{
  background: url("data:image/png;base64,")  no-repeat center center;
  background-color: #0099ff;
  background-size: 45rpx;
  padding: 10rpx 90rpx;
  border-radius: 0px 6px 6px 0px;
}
.input1{
  border: solid 1px #0099ff;
  height: 90rpx;
  width: fit-content;
  flex-grow: 1;
  padding-left: 20rpx;
  box-sizing:border-box;
  flex-shrink: 1; 
}
.placesty{color: #bbbac4;}

.psty1{
  display: flex;
  justify-content: space-between;
}
.psty2{
  display: flex;
  justify-content: space-between;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  color: #535252;
  background-color: #f9f9f9;
  font-size:28rpx;
}
.text1,.text2{
  text-align: center;
}
.text1{width: 60%;}
.text2{width: 40%;}
.text3{
  color: #7f7f7f;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  flex-shrink: 1;
  align-self:center;
  padding-left: 40rpx;
  background: url("data:image/png;base64,") no-repeat 0 center/28rpx;
}
.text4{
  color: #fff;
  	background-image: linear-gradient(90deg, 
		#ffa700 0%, 
		#ffb427 100%);
	border-radius: 6rpx;
  padding: 15rpx 30rpx;
  margin-left: 30rpx;
}
.psty3{
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #f8f8f8;
  margin: 30rpx 20rpx;
}
.psty4{
  text-align: center;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}