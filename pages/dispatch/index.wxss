.psty5{
  position: relative;
}
.psty5::before,.psty5::after{
  position: absolute;
  content: ' ';
  width: 40rpx;
  height: 4rpx;
  background-color: #ff596a;
  top: 30rpx;
  border-radius: 30%;
}
.psty5::before{left: -60rpx;}
.psty5::after{right: -60rpx;}
.dipa-inf2{
