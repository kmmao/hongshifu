/**index.wxss**/
page{font-size: 28rpx;background-color: #fff;height: 100%}
.pheight{height: 100%}
map{width: 100%;height: 100%;
position: relative;}
input{height: 3.5em;font-size: 28rpx;}
.appoint{
  position: fixed;
  width: calc(100% - 60rpx);
  bottom: 30rpx;
  left: 30rpx;
  right: 30rpx;
  box-shadow: 0px 2px 20px 0px 	rgba(153, 153, 153, 0.2);
  border-radius: 20rpx;
  background-color: rgba(255, 255, 255, .85)
}
.btn1{
  margin: 20rpx 30rpx 40rpx;
  text-align: center;
  background-color: #ff596a;
	font-size: 32rpx;
  font-weight: 400;
	color: #ffffff;
  border-radius: 20px;
  line-height: 3.2;
}
.my-icon{
  position:fixed;
  top:30rpx;
  left: 30rpx;
  padding: 20rpx 30rpx 20rpx 80rpx;
  font-size: 28rpx;
  color: #639aff;
  border-radius: 20rpx;
  background:#fff url("data:image/svg+xml;base64,") no-repeat 30rpx center/30rpx;
}
.htitle1{
  display: flex;
  justify-content:space-around;
  align-items: center;
  line-height: 3.5;
  flex-wrap:nowrap;
}
.psty0,.psty1{
  border-radius: 20rpx 20rpx 0 0;
  background-color: transparent;
  flex:auto;
  text-align: center;
  } 
.psty5{
  border-radius: 20rpx 20rpx 0 20rpx;
  background-color: transparent;
  margin: 30rpx 30rpx 20rpx; 
}
.psty0.active,.psty1.active{ background-color: #fff;}
.psty0.active .psty5,.psty1.active .psty5{background-color: #eee;color: #ff596a;}
.htitle1>view:first-child .psty5::before{
  width: 70rpx;
  height:50rpx;
  display: inline-block;
  content: "";
  background-repeat: no-repeat;
  background-position: left center;
  background-image: url("data:image/svg+xml;base64,");
vertical-align:-12rpx;
background-size: 50rpx; 
}
.htitle1>view:first-child.active .psty5::before{
  background-image: url("data:image/svg+xml;base64,");
}
.htitle1>view:nth-child(2n+1) .psty5{border-radius: 20rpx 20rpx 0 20rpx;}
.htitle1>view:nth-child(2n) .psty5 {border-radius: 20rpx 20rpx 20rpx 0;}






.psty3,.psty4,.psty6{
  position: relative;
  margin-left: 40rpx;
  margin-right: 30rpx;
  line-height: 3;
  padding-left: 35rpx;
}
.psty6{
  background: url("data:image/svg+xml;base64,") no-repeat left center;
  background-size: 28rpx;
}
.psty3::before,.psty4::before{
  position: absolute;
  content: " ";
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  top:45rpx;
  left: 0;
}
.psty4::before{background-color: #44bfa3;}
.psty3::before{background-color: #fe5252;}
.psty3{border-bottom: 1rpx solid #f0f0f0;}


.psty2 .psty5{border-radius: 20rpx 20rpx 20rpx 0;}
.psty1,.psty2{
  flex:auto;
  text-align: center;
}
.psty1.active,.psty2.active{ background-color: #fff;}
.psty1.active .psty5,.psty2.active .psty5{background-color: #eee;color: #ff596a;}
.appoint-cont1,.appoint-cont3{
  background-color: #fff;
  border-bottom: 1rpx solid #fff;
  }

.appoint-cont1{border-radius: 0 20rpx 20rpx 20rpx;}
.appoint-cont3{border-radius: 20rpx 0 20rpx 20rpx;}
.psty1 .icon1{
  vertical-align:-12rpx;
  background-size: 50rpx;
  width: 70rpx;
  height:50rpx;
  display: inline-block;
  background-repeat: no-repeat;
  background-position: left center;
  background-image: url("data:image/svg+xml;base64,");
}
.psty1.active .icon1{
  background-image: url("data:image/svg+xml;base64,");
}

.header {
    margin: 90rpx 0 60rpx 0rpx;
    text-align: center;
    width: 100%;
}
 
.header image {
    width: 248rpx;
    height: 248rpx;
}
 
.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
    text-align: center;
}
 
.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
 
.bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;

}
.img-map{
	width: 54rpx;
	height: 54rpx;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%); 
}
.img-map2{
  width: 54rpx;
	height: 54rpx;
	position: absolute;
	top: 20%;
	right: 10%;
	transform: translate(-50%,-50%); 
}