/* pages/travel/index.wxss */
page{height: 100%}
map{width: 100%;height: 100%;}
.travel-cont{
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: -7px 53px 68px 0px #888585;
  position: fixed;
  bottom: 20rpx;
  left: 30rpx;
  right: 30rpx;
  width:calc(100% - 60rpx);
  padding: 25rpx;
  box-sizing:border-box;
  overflow: hidden;
}
.travel-cont1{
  position: relative;
}
.travel-cont::after{
  position: absolute;
  width: 300rpx;
  height: 300rpx;
  background:radial-gradient(#fff, #ebeaea);
  border-radius: 50%;
  content: ' ';
  top: -150rpx;
  right: -150rpx; 
}
.psty1{
  position: absolute;
  top: 0rpx;
  right: -30rpx;
  color: #ff596a;
  z-index: 1;
  line-height: 75rpx;
  font-size: 24rpx;
  width: 150rpx;
  text-align: center;
}
.font1{font-size: 42rpx;}
.psty2{
  border-bottom: 1rpx solid #f2f2f2;
  font-size: 32rpx;
  margin-left: -30rpx;
  font-weight: bold;
  background: url("data:image/svg+xml;base64,") no-repeat 30rpx center/40rpx;
  padding: 25rpx 60rpx 35rpx 95rpx;
}
.psty4,.psty3{
  position: relative;
  margin-left: 60rpx;
  padding-top: 35rpx;
  padding-bottom: 35rpx;
}
/* .psty6,.psty7{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-right: 50rpx;
} */
.psty3{
  margin-right: 150rpx;
}
.psty3::before,.psty4::before{
  position: absolute;
  content: " ";
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  top:50rpx;
  left: -45rpx;
}
.psty4::before{background-color: #44bfa3;}
.psty3::before{background-color: #fe5252;}
.psty4{
  border-top: 1rpx solid #f2f2f2;
  display: flex;
  justify-content:space-between;
  align-items:center;
}
.psty5{
  background-color: #ff596a;
  border-radius: 20rpx;
  color: #fff;
  padding: 15rpx 50rpx;
  font-size: 26rpx;
  width: 90rpx;
}
.psty6{
  margin-top: -25rpx;
  flex-shrink: 1;
  margin-right: 25rpx;
}



