  background: url("data:image/svg+xml;base64,") no-repeat right center;
  background-size: 25rpx;
}
.tex1{
  padding-top: 20rpx;
}
.pcont2,.pcont3{margin-top: 20rpx;}
.tex2,.tex1{
  position: relative;
  text-indent: 35rpx;
  font-size: 28rpx;
  white-space: nowrap; 
  overflow: hidden;
  text-overflow:ellipsis;
}
.tex2{
    margin-right: 26rpx;
    min-width: 15rpx;
    min-height: 47rpx;
}
.tex1::before,.tex2::before{
  position: absolute;
  content: " ";
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  left: 0rpx;
}
.tex1::before{background-color: #44bfa3;top: 38rpx;}
.tex2::before{background-color: #fe5252;top: 18rpx;}

