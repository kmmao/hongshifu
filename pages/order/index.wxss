page{
  background-color: #f2f2f2;
}
.order-inf{
  margin: 15rpx 20rpx;
  background-color: #fff;
  font-size: 28rpx;
  color: #666;
  border-radius: 10rpx;
}
.pcont1,.pcont3,.pcont4,.pcont2{
  display: flex;
  justify-content:space-between;
  padding-left: 35rpx;
  padding-right: 35rpx;
  align-items: center;
}
.pcont1{
  padding-top: 40rpx;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #f2f2f2;
  position: relative;
  font-size: 28rpx;
}
.pcont1::after,.pcont1::before{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  background-color: #f2f2f2;
  content: " ";
  bottom: -15rpx;
  border-radius: 50%;
}
.pcont1::after{left: -15rpx}
.pcont1::before{right: -15rpx;}
.tex6{
  color: #000;
  font-size: 28rpx;
  }
